#!/Users/allenbsf/Code/provider_payments_api/env/bin/python3.6
# EASY-INSTALL-ENTRY-SCRIPT: 'csvkit==1.0.2','console_scripts','csvpy'
__requires__ = 'csvkit==1.0.2'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('csvkit==1.0.2', 'console_scripts', 'csvpy')()
    )
